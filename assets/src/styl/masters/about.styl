.team_carousel {
    position: relative;

    .slick-track {
        padding: 2rem 0;
    }

    .slick-prev, .slick-next {
        height: 40px;
        width: 40px;
        border: none;
        background-color: transparent;
        color: transparent;
        position: absolute;
        outline: none;
        z-index: 99999999;
    }

    .slick-prev {
        background: url('../img/arrow_left.png') no-repeat;
        background-size: contain;
        top: 50%;
        left: -3%;
        transform: translate(-50%, -50%);
    }

    .slick-next {
        background: url('../img/arrow_right.png') no-repeat;
        background-size: contain;
        top: 50%;
        left: 103%;
        transform: translate(-50%, -50%);
    }
}

.about_team_box {
    background: #FFFFFF;
    box-shadow: 0px 0px 15px rgba(24, 33, 44, 0.15);
    border-radius: 8px;
    margin: 0 1rem;

    .linkedin {
        width: 17.37px;
        height: 16px;
        color: #3C4147;
    }

    .linkedin {
        margin-top: 0.9rem;
    }

    .mail {
        width: 21.87px;
    }

    .carousel_img_fix {
        border-radius: 50%;
        height: 120px;
        width: 120px;
    }

    .person {
        font-style: normal;
        font-weight: 600;
        font-size: 20px;
        line-height: 32px;
        text-align: center;
        color: #53A1D8;
    }
}

.sec_subtitle, .details_about_section .sec_subtitle {
    font-weight: 300;
    font-size: 20px;
    line-height: 48px;
    color: #53a1d8;
    padding-top: 1rem;
}

.about_cta {
    width: 200px;
    height: 62.63px;
    background: #53A1D8;
    border-radius: 6px;
    font-weight: 600;
    font-size: 22px;
    line-height: 45px;
    color: #FFFFFF;

    &:hover {
        color: white;
        text-decoration: none;
    }
}

.logo {
    height: 60px;
    width: 60px;
}

.vene_awards {
    padding 2rem 0;
}

.awards-title {
    display: flex;
    justify-content: center;

    .vene_award {
      align-self: center;
      height: 40px;
      margin-top: -8px;
      width: 40px;
    }
}

.awards-container {
    display: flex;
    justify-content: space-around;
    @media (max-width: 475px) {
      flex-wrap: wrap;
    }

    .award-box {
      margin: 15px 0;

      img {
          max-width: 200px;
          width: 100%;
      }
    }

}


